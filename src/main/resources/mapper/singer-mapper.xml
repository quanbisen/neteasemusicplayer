<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="model.SingerMapper">
    <select id="findSinger" parameterType="String" resultMap="singerResultMap" >
        select name,image_url from singers where name=#{value};
    </select>
    <resultMap id="singerResultMap" type="Singer">
        <result property="name" column="name"></result>
        <result property="imageURL" column="image_url"></result>
    </resultMap>
</mapper>